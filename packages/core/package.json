{
  "name": "@solid-memo/core",
  "version": "0.1.0",
  "main": "src/index.ts",
  "types": "index.ts",
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "verify": "npm run test",
    "test:unit": "vitest run test/unit/",
    "test:integration": "vitest run test/integration/",
