  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "dependencies": {
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.1.2",
    "vitest": "^3.1.2"
  }
}
