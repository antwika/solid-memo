{
  "name": "@solid-memo/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run test/unit/",
    "test:integration": "vitest run test/integration/",
    "test:unit:coverage": "vitest run --coverage test/unit/",
    "test:unit:watch": "vitest",
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "verify": "npm run lint && npm run check && npm run test && npm run build"
  },
  "dependencies": {
    "@inrupt/solid-client": "^2.1.2",
    "@inrupt/solid-client-authn-browser": "^3.0.0",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@solid-memo/core": "*",
    "@solid-memo/inrupt-solid-client-repository": "*",
    "@t3-oss/env-nextjs": "^0.13.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.552.0",
    "next": "^15.2.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.2",
    "react-redux": "^9.2.0",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.1.0",
    "tw-animate-css": "^1.2.5",
    "uuid": "^13.0.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitest/coverage-v8": "^4.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "tailwindcss": "^4.0.15",
    "ts-loader": "^9.5.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.27.0",
    "vitest": "^4.0.0",
    "vitest-when": "^0.9.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.2"
  },
  "packageManager": "npm@11.6.2"
}
