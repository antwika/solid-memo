{
  "name": "app-playground",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "dev": "next dev",
    "build": "next build",
    "test": "jest",
    "start": "next start",
    "lint": "npm run lint:src && npm run lint:tests",
    "lint:src": "next lint --dir src",
    "lint:tests": "next lint --dir tests",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\""
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "axios": "1.6.7",
    "n3": "1.17.2",
    "next": "13.1.6",
    "next-auth": "4.24.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.0.1",
    "swr": "2.2.4",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.1.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "14.2.1",
    "@types/jest": "^29.2.1",
    "@types/n3": "^1.10.4",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-next": "^13.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.3",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.14",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^3.1.6",
    "typescript": "^5.0.0"
  }
}
